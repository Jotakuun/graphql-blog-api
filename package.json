    "build:prod": "SET NODE_ENV=production&& babel src -d dist",
    "server": "node dist/server.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jotakuun/graphql-blog-api.git"
  },
